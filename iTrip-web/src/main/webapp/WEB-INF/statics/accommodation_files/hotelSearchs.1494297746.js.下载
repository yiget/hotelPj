  url: '/hotel/getAjaxRoomPrice.ydd',
  type: 'GET',
  data: 'hotelIds='+$j("#hotelIds").text()+'&starttime='+$j("#starttime").val()+'&endtime='+$j("#endtime").val()+'&pricefrom='+$j("#pricefrom").val()+'&priceto='+$j("#priceto").val()+'&sort='+$j("#sort").val(),
