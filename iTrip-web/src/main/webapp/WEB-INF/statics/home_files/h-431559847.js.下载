function checkAll(name){var el=document.getElementsByTagName('input');var len=el.length;for(var i=0;i<len;i++){if((el[i].type=="checkbox")&&(el[i].name==name)){el[i].checked=true;$(el[i].id+'-box').addClassName('selected');}}
