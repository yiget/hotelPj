function clearAll(name){var el=document.getElementsByTagName('input');var len=el.length;for(var i=0;i<len;i++){if((el[i].type=="checkbox")&&(el[i].name==name)){el[i].checked=false;$(el[i].id+'-box').removeClassName('selected');}}
el=null;}
