    $j("#popupUseremail-errMsg").html("");
    $j("#popupUserpassword-errMsg").html("");
    $j("#popupLogin-errMsg").html("");
    if(loginname.length < 1) {
      $j("#popupUseremail-errMsg").html("请输入用户名！");
      return false;
    }
    if(loginpassword.length < 1) {
      $j("#popupUserpassword-errMsg").html("请输入密码！");
      return false;
    }
    if($j("#popupRememberMe").attr("checked") == 'checked') {
      rememberMe = true;
    }
    $j.get("Login",
               {"ajaxloginemail" : loginname,
                 "ajaxloginpassword64" : loginpassword64,
                 "ajaxverifycode" : verifycode,
                 "ajaxRememberMe" : rememberMe
