              success: function(msg) {
                var status = msg.sta;
                if(status == 'field') {
                  $j("#popupLogin-errMsg").text(msg.message);
                  if(msg.verifycode && msg.verifycode != '') {
                    window.location.href="/user/login.ydd?rd=" + getRdHref();
                  }
                } else {
                  $j("#top_land_show").html(msg.message);
                  if(loginSuccessCallback) {
                    loginSuccessCallback();
                  }
                  if(loginSuccessReload){
                    window.location.reload(true);
                  }
                }
                return false;
              }
            });
  }

  function checkPopupLoginForm(form) {
    var loginname = $j("#popupUseremail").attr("value").trim();
    var loginpassword = $j("#popupUserpassword").attr("value").trim();
    var loginpassword64 = BASE64.encode(loginpassword);
    var verifycode="";
