              success: function(msg) {
                var status = msg.sta;
                if(status == 'field') {
                  $j("#popupLogin-errMsg").text(msg.message);
                  if(msg.verifycode && msg.verifycode != '') {
                    window.location.href="/user/login.ydd?rd=" + getRdHref();
                  }
                } else {
                  $j("#top_land_show").html(msg.message);
                  if(loginSuccessCallback) {
                    loginSuccessCallback();
                  }
                  if(loginSuccessReload){
                    window.location.reload(true);
                  }
                }
                return false;
              }
            });
  }

  function checkPopupLoginForm(form) {
    var loginname = $j("#popupUseremail").attr("value").trim();
    var loginpassword = $j("#popupUserpassword").attr("value").trim();
    var loginpassword64 = BASE64.encode(loginpassword);
    var verifycode="";
    var rememberMe = false;
    $j("#popupUseremail-errMsg").html("");
    $j("#popupUserpassword-errMsg").html("");
    $j("#popupLogin-errMsg").html("");
    if(loginname.length < 1) {
      $j("#popupUseremail-errMsg").html("请输入用户名！");
      return false;
    }
    if(loginpassword.length < 1) {
      $j("#popupUserpassword-errMsg").html("请输入密码！");
      return false;
    }
    if($j("#popupRememberMe").attr("checked") == 'checked') {
      rememberMe = true;
    }
    $j.getJSON("http://www.yododo.com/go/app/cn/hotelUserAutoLogin.ydd?callback=?",
               {"ajaxloginemail" : loginname,
                 "ajaxloginpassword64" : loginpassword64,
                 "ajaxverifycode" : verifycode,
                 "ajaxRememberMe" : rememberMe
               }, function(json){
          if(json.loginstatus == 'loginsuccess') {
            popupSyncLogin(json.loginname, json.loginpwd,rememberMe);
          } else {
            $j("#popupLogin-errMsg").text(json.errorMsg);
            if(json.verifycode == 'true' || json.loginstatus == 'verifyfailed'){
              window.location.href="/user/login.ydd?rd=" + getRdHref();
            }
          }
        }
    );
    return false;
  }

  function showPopupLogin(reloadAftersuccess,successCallback) {
    loginSuccessReload = reloadAftersuccess;
    loginSuccessCallback = successCallback;
    $j.blockUI({message:$j("#popupLogin"),css:{top: '50%','margin-top': -150, left: '50%', 'margin-left': -275, border:'none', 'background-color': 'transparent', cursor: 'default', width:570, height:'auto', 'text-align':'left'},overlayCSS:{cursor: 'default'}});
