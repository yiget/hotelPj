  function popupSyncLogin(loginname, loginpassword, remeber){
    $j.ajax({url: '/user/ajaxHotelLogin.ydd',
              type: 'POST',
              data: 'ajaxloginemail='+loginname+"&ajaxloginpassword="+loginpassword+"&rememberMe="+remeber,
              dataType: 'json',
