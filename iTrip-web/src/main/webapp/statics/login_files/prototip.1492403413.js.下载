var ProtoTip={xCord:0,yCord:0,tip:null,tagClass:"tooltip",attachTooltipBehavior:function(_1){if(!document.getElementById||!document.createElement||!document.getElementsByTagName){return;}var _2=$("protoTip");if(_2==null){_2=document.createElement("div");_2.id="protoTip";_2.style.display="none";document.body.appendChild(_2);_2.innerHTML="";}$j(document).unbind("mousemove",ProtoTip.updateXY);$j(document).bind("mousemove",ProtoTip.updateXY);if(document.captureEvents){document.captureEvents(Event.MOUSEMOVE);}var _3=Element.select($(_1),"."+ProtoTip.tagClass);for(var j=0,element;element=_3[j];j++){ProtoTip.addTooltip(element);}},addTooltip:function(_5){if(_5&&!_5.getAttribute("tip")){$j(_5).mouseover(ProtoTip.tipOver);$j(_5).mouseout(ProtoTip.tipOut);_5.setAttribute("tip",_5.title);_5.removeAttribute("title");}},updateXY:function(e){ProtoTip.xCord=Event.pointerX(e);ProtoTip.yCord=Event.pointerY(e);if($("protoTip").style.display=="block"){ProtoTip.tipShow(false);}},tipOut:function(e){if(window.tID){clearTimeout(tID);}if(window.opacityID){clearTimeout(opacityID);}$("protoTip").style.display="none";},tipOver:function(e){var _9=Event.element(e);if(_9){var _a=findParentElementByClassName(ProtoTip.tagClass,_9);ProtoTip.tipOverWithData(_a.getAttribute("tip"));_a=null;}},tipOverWithData:function(_b){ProtoTip.tip=_b;window.tID=setTimeout("ProtoTip.tipShow(true)",100);},tipShow:function(_c){var _d=$("protoTip");if(_c){var _e="";var _f="";var _10="";var _11=ProtoTip.tip.split(";");for(i=0;i<_11.length;i++){var _12=_11[i];var _13=_12.split(":");if(_13.length==2){_f=_13[0];_10=_13[1];}else{_10=_12;}_10=(stripHTML(_10).trim().length>380?_10.substring(0,380)+"...":_10);_e+="<div class='tip-header'>"+_f+"</div><div class='tip-text'>"+_10+"</div>";}_d.innerHTML=_e;}_d.style.display="block";var top=ProtoTip.yCord+15;var _15=ProtoTip.xCord+10;if(ProtoTip.willOverflowHorizontal(_d.offsetWidth+_15)){_d.style.right="20px";}else{_d.style.left=_15+"px";}if(ProtoTip.willOverflowVertical(_d.offse